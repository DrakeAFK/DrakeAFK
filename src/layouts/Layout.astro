---
interface Props {
title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8" />
	<meta name="description" content="DrakeAFK (Drake Hopkins) Personal Website">
	<meta name="viewport" content="width=device-width" />
	<link rel="icon" type="icon" href="/favicon.ico" />
	<meta name="generator" content={Astro.generator} />
	<title>{title}</title>
</head>

<body>
	<slot />
</body>

</html>